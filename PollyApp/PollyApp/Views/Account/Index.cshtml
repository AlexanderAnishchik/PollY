@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Dashboard | Polly</title>
    <link href="~/Content/css/animate.css" rel="stylesheet">
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/custom.css" rel="stylesheet">
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
    <link rel="icon" href="~/favicon.ico" type="image/x-icon">
    <script src="~/Scripts/jquery-1.11.3.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-ui/ui-bootstrap.js"></script>
    <script src="~/Scripts/angular-ui/ui-bootstrap-tpls.min.js"></script>
    <script src="~/Scripts/App/Modules/ngAnimation.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/front-end/custom.js"></script>
    <link href="~/Content/css/green.css" rel="stylesheet" />
    <script src="~/Scripts/angular-aria.js"></script>
    <script src="~/Scripts/angular-animate.js"></script>

    <script src="~/Scripts/App/app.js"></script>
    <script src="~/Scripts/App/Services/headerKeeperService.js"></script>
    <script src="~/Scripts/App/Controllers/headerController.js"></script>
    <script src="~/Scripts/App/Controllers/accountController.js"></script>
    <script src="~/Scripts/angular-material/angular-material-mocks.js"></script>
    <script src="~/Scripts/angular-material/angular-material.min.js"></script>
</head>
<body class="nav-md" ng-app="Polly">

    <div class="container body">


        <div class="main_container">

            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">

                    <div class="navbar nav_title site_title" style="border: 0;">

                        <a href="/" class=""><span><img src="~/Content/images/pollylogogreen.png" /></span></a>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>

                    </div>
                    <div class="clearfix"></div>

                    <!-- menu prile quick info -->
                    <div class="profile" ng-controller="headerController as headCtrl" ng-init="headCtrl.init()">
                        <div class="profile_pic">
                            <img src="~/Content/images/Users/{{headerData.user.logo}}" alt="{{headerData.user.FirstName}} {{headerData.user.LastName}}" class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Welcome,</span>
                            <h2 ng-bind-template="{{headerData.user.FirstName}} {{headerData.user.LastName}}"></h2>
                        </div>
                    </div>
                    <!-- /menu prile quick info -->
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">

                        <div class="menu_section">
                            <h3>General</h3>
                            <ul class="nav side-menu">
                                <li>
                                    <a><i class="fa fa-home"></i> Home <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="index.html">Dashboard</a>
                                        </li>
                                        <li>
                                            <a href="index2.html">Dashboard2</a>
                                        </li>
                                        <li>
                                            <a href="index3.html">Dashboard3</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-edit"></i> Polls <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="index.html">All polls</a>
                                        </li>
                                        <li>
                                            <a href="index.html">Charts</a>
                                        </li>
                                        <li>
                                            <a href="index.html">API</a>
                                        </li>

                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-desktop"></i> Account <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="index.html">Profile</a>
                                        </li>
                                        <li>
                                            <a href="index.html">Settings</a>
                                        </li>
                                        <li>
                                            <a href="index.html">Community</a>
                                        </li>

                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-table"></i> Tables <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="tables.html">Tables</a>
                                        </li>
                                        <li>
                                            <a href="tables_dynamic.html">Table Dynamic</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-bar-chart-o"></i> Data Presentation <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="chartjs.html">Chart JS</a>
                                        </li>
                                        <li>
                                            <a href="chartjs2.html">Chart JS2</a>
                                        </li>
                                        <li>
                                            <a href="morisjs.html">Moris JS</a>
                                        </li>
                                        <li>
                                            <a href="echarts.html">ECharts </a>
                                        </li>
                                        <li>
                                            <a href="other_charts.html">Other Charts </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>


                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small text-center">
                        @*Copyright &copy;
                            <script>
                                var now = new Date();
                                document.write(now.getFullYear());
                            </script>*@
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            <!-- /top navigation -->
            <!-- page content -->
            <div ng-controller="accountController as mainCtrl" ng-init="mainCtrl.init()" class="right_col" role="main">
                <div ng-include src="mainCtrl.currentTab">
                </div>
            </div>
            <!-- /page content -->

        </div>

    </div>

    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>

</body>
</html>

